{
    "name": "webhappens/traverser",
    "description": "Easily traverse nested object structures without worrying about recursion.",
    "keywords": [
        "webhappens",
        "tree",
        "traversal"
    ],
    "homepage": "https://github.com/webhappens/traverser",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Gurney",
            "email": "ben@webhappens.co.uk",
            "homepage": "https://webhappens.co.uk",
            "role": "Developer"
        },
        {
            "name": "Sam Leicester",
            "email": "sam@webhappens.co.uk",
            "homepage": "https://webhappens.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "tightenco/collect": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.15|^8.4|^9.0",
        "symfony/var-dumper": "^4.3.4|^5.0"
    },
    "autoload": {
        "psr-4": {
            "WebHappens\\Traverser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebHappens\\Traverser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
