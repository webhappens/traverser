{
    "name": "webhappens/traverser",
    "description": "A Laravel package for traversing nested object structures",
    "keywords": [
        "webhappens",
        "tree",
        "traversal"
    ],
    "homepage": "https://github.com/webhappens/traverser",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Gurney",
            "email": "ben@webhappens.co.uk",
            "homepage": "https://webhappens.co.uk",
            "role": "Developer"
        },
        {
            "name": "Sam Leicester",
            "email": "sam@webhappens.co.uk",
            "homepage": "https://webhappens.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "tightenco/collect": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "WebHappens\\Traverser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebHappens\\Traverser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
